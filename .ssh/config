# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# This won't be added again if you remove it.
Include ~/.orbstack/ssh/config

Host *
ForwardAgent yes
#UseRoaming No
#UseKeychain yes
AddKeysToAgent yes
IdentityFile ~/.ssh/id_ed25519

Include ~/.ssh/control

Host boojum
HostName boojum.manxome.org
User rjbs

Host snark
HostName snark.manxome.org
User rjbs

Host dodo
HostName dodo.manxome.org
User rjbs

Host synergy
HostName s.yner.gy
User synergy

Host fmd
HostName phl-jump-01.messagingengine.com
User rjbs
SendEnv FM_*
# Secretive's Secret Agent
IdentityAgent /Users/rjbs/Library/Containers/com.maxgoedjen.Secretive.SecretAgent/Data/socket.ssh

Host gitlab.fm
IdentityAgent /Users/rjbs/Library/Containers/com.maxgoedjen.Secretive.SecretAgent/Data/socket.ssh

Host box
HostName rjbs.fastmailvm.com
UserKnownHostsFile /dev/null
StrictHostKeyChecking no
SendEnv FM_*

Host *.rjbs.fastmailvm.com
UserKnownHostsFile /dev/null
StrictHostKeyChecking no
SendEnv FM_*

Host github.com
IdentityAgent /Users/rjbs/Library/Containers/com.maxgoedjen.Secretive.SecretAgent/Data/socket.ssh

Host pause
HostName pause3.develooper.com
